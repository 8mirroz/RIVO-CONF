{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://rivo.example/schema/structure-graph.schema.json",
  "title": "StructureGraph",
  "type": "object",
  "required": ["rootNode", "edges"],
  "properties": {
    "rootNode": {"type": "string"},
    "edges": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["from", "to"],
        "properties": {
          "from": {"type": "string"},
          "to": {"type": "string"},
          "type": {"type": "string"}
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
